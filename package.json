{
  "name": "ai-vision-cli",
  "version": "1.0.0",
  "description": "Enterprise-grade CLI for AI-powered image analysis with advanced reliability features",
  "main": "dist/index.js",
  "bin": {
    "ai-vision": "./bin/ai-vision"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run lint && npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "vision",
    "image-analysis",
    "cli",
    "gemini",
    "vertex-ai",
    "object-detection",
    "enterprise",
    "reliable",
    "circuit-breaker",
    "retry-logic"
  ],
  "author": "Major Mark",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@google/genai": "^0.3.0",
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "sharp": "^0.33.5",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/majormark/ai-vision-cli.git"
  },
  "bugs": {
    "url": "https://github.com/majormark/ai-vision-cli/issues"
  },
  "homepage": "https://github.com/majormark/ai-vision-cli#readme"
}
